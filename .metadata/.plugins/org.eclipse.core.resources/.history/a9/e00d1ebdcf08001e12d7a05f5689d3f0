public class Hello {

	// 프로그램 진입점
	public static void main(String[] args) {
	
		// 기본출력
		System.out.println("Hello world~");
		System.out.println("Hello Java~");
		
		// 한줄출력
		System.out.println("Hello ");
		System.out.println("Korea~");
		
	} 
	
	
}
